def sum_of_distinct_elements(set1, set2):
    # Combine both sets and get distinct elements using set union
    distinct_elements = set(set1) | set(set2)
    # Return the sum of distinct elements
    return sum(distinct_elements)

# Example usage:
set1 = [1, 2, 3, 4]
set2 = [3, 4, 5, 6]
result = sum_of_distinct_elements(set1, set2)
print(result)  # Output: 21


procedure dot-product(v1, v2):
    initialize product_sum to 0
    for i from 0 to length(v1) - 1:do
        ps = ps + (v1[i] * v2[i])
    end for
    return ps
    end procedure


    procedure check-orthogonal(pairs):
    for each pair in pairs(v1, v2) in pairs do
    ps = dot-product(v1, v2)
    if ps == 0 then
        print " orthogonal"
    else
        print " not orthogonal"
    end if
    end for
    end procedure


